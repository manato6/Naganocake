<h1>商品編集</h1>

<%= form_with model: Item.new, url: admin_items_path do |f| %>

<h4>商品画像</h4>
<%= f.file_field :image%>

<h4>商品名</h4>
<%= f.text_field :name%>

<h4>商品説明</h4>
<%= f.text_field :introduction%>

<h4>税抜き価格</h4>
<%= f.text_field :price%>円

<div class="actions">
    <%= f.submit "変更を保存",class:"btn btn-success"%>
  </div>

<% end %>