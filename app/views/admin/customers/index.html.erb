<div class="container">
  <div class="row">
   <h1>会員一覧</h1>
   </div>
   
<table class="table table-hover table-inverse">
<thead>
    <tr>
      <th>会員ID</th>
      <th>会員名</th>
      <th>メールアドレス</th>
    </tr>
  </thead>
  <tbody>

  <% @customer.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= link_to customer.last_name+customer.first_name,admin_customer_path(customer) %></td>
      <td><%= customer.email %></td>
    </tr>
  <% end %>
  </tbody>
  </table>
  </div>